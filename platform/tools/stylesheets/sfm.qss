QSplitter#AnimationSetEditorVerticalSplitter
{
	background-color: rgb( 38, 38, 39 );
	margin: 0px;
	border: 0px;
	width: 6px;
	height: 6px;
}

QSplitter#CQDagPickerDialogSplitter
{
	background-color: rgb( 54, 54, 54 );
	margin: 0px;
	border: 0px;
	width: 6px;
	height: 6px;
}

 

QSplitter#AnimationSetEditorVerticalSplitter::handle:vertical {
     height: 8px;
	image: url(tools:images/sfm/ase_splitter_handle_vertical.png);
 }
 
 
 
#CQAnimationSetPresetSliderBox QTabBar
{
	background-color: rgb( 38, 38, 39 );
}

#CQAnimationSetPresetSliderBox QTabBar::tab
{
	background-color: rgb( 54, 54, 54 );
	border: 1px solid rgb(82, 82, 82 );
	border-bottom-color: rgb(44, 44,44, 40%);
	color: rgb(182, 182, 182);
	min-height: 10px;
	min-width: 10px;
	image: url(tools:images/valve_base/empty.png);
	padding-left: 4px;
    padding-right: 4px;
    padding-top: 2px;
}

#CQAnimationSetPresetSliderBox QTabBar::tab:!selected 
{
     margin-top: 2px; 
}
 
#CQAnimationSetPresetSliderBox QTabBar::tab:selected 
{
	color: rgb(200, 200, 200);
    border-color: rgb( 182, 182,182, 40% );
    border-bottom-color: rgb(44, 44,44, 40%);
}

