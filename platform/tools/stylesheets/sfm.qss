/* Specifics for the SFM */



CQExportShotListTableWidget
{
	qproperty-progressTextColor: rgb( 182, 182, 182 );
	qproperty-doneTextColor: rgb( 138, 149, 186 );
}

CQConsoleTextEdit
{
	background-color: rgb( 44, 44, 44);
	selection-color: rgb(255, 252, 229 );
	selection-background-color: rgb( 0, 0, 255 );

	font-family: "Courier";
	font-size: 11px;
}

CQConsoleTextEdit:focus
{
	border-color: rgb( 39, 39, 39 );
	background-color: rgb(39, 39, 39 );
}


CQPythonConsoleTextEdit
{
	background-color: rgb( 44, 44, 44);
	selection-color: rgb(255, 252, 229 );
	selection-background-color: rgb( 0, 0, 255 );
	border-radius: 0px;
	font-family: "Courier";
	font-size: 11px;
}

CQPythonConsoleTextEdit:focus
{
	border-radius: 0px;
	border-color: rgb( 39, 39, 39 );
	background-color: rgb(39, 39, 39 );
}


CQPythonTextEdit
{
	border-image: url(tools:images/valve_base/frame_popupdialog.png) 4;
	
	qproperty-currentLineHighlightColor: rgb( 0, 0, 0 );
	qproperty-lineNumberBackgroundColor: rgb( 60, 60, 60 );
	qproperty-lineNumberColor: rgb( 100, 100, 100 );

	font-family: "Courier";
	font-size: 11px;

}

CQGraphEditorLineEdit[isMultiple="true"]
{
	border-image: url(tools:images/valve_base/frame_multiple.png) 3;
}

