QMenu
{
	border: 2px groove;
	border-color: rgb(255, 255, 255) ;
	background-color: rgb(245, 245, 245);
}




QComboBox[editable="true"]
{
	padding-left: 7px;
	border: 2px groove;
	border-color: rgb(96, 96, 96) ;
	border-radius: 6px;

	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(222, 222, 222, 255), stop:1 rgba(233, 233, 233, 255));
	color: rgb( 58, 58, 58, 255 );

}

QComboBox[editable="true"][roundTop="true"]
{
	border-bottom-left-radius: 0px;
	border-bottom-right-radius: 0px;

	border-bottom-width: 3px;
	border-bottom-style: none;
	padding-bottom: 2px;
}

QComboBox[editable="true"][roundMid="true"]
{
	border-top-left-radius: 0px;
	border-top-right-radius: 0px;
	border-bottom-left-radius: 0px;
	border-bottom-right-radius: 0px;

	border-top-width: 1px;
	border-top-style: solid;
	border-bottom-width: 0px;
	border-bottom-style: none;
	border-radius: 0px;
	padding-top: 1px;
	padding-bottom: 2px;
}

QComboBox[editable="true"][roundBottom="true"]
{
	border-top-left-radius: 0px;
	border-top-right-radius: 0px;

	border-top-width: 1px;
	border-top-style: solid;
	padding-top: 1px;
}

QComboBox[editable="true"]:hover
{
	/*background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(232, 232, 232, 255), stop:1 rgba(243, 243, 243, 255));
	*/
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(202, 202, 202, 255), stop:1 rgba(213, 213, 213, 255));
}

QComboBox::drop-down[editable="true"]
{
	border: none;
	border-left-width: 2px;
	right: 2px;
	width: 20px;
}









QCheckBox {
	border: 1px solid;
	border-color: rgb( 22, 98, 224, 0) ;
}


QCheckBox::indicator:checked:hover {
border: 2px solid;
border-color: rgb( 222, 98, 224, 0) ;
}



QCheckBox::indicator {
}


QCheckBox::indicator:unchecked {
	background-color: rgba( 86, 86, 86, 255);
	image: url(images/propertyeditor/checkbox_unchecked.png);
}


QCheckBox::indicator:unchecked:hover {
	image: url(images/propertyeditor/checkbox_unchecked_hover.png);
}

QCheckBox::indicator:unchecked:pressed {
	image: url(images/propertyeditor/checkbox_unchecked_pressed.png);
}

QCheckBox::indicator:checked {
	image: url(images/propertyeditor/checkbox_checked.png);
}

QCheckBox::indicator:checked:hover {
	image: url(images/propertyeditor/checkbox_checked_hover.png);
}

QCheckBox::indicator:checked:pressed {
	image: url(images/propertyeditor/checkbox_checked_pressed.png);
}

QCheckBox::indicator:focus {
border-color: rgb( 22, 98, 224, 128) ;
}






